{
  "name": "quasar-project",
  "version": "0.0.1",
  "description": "A Quasar Project",
  "productName": "Quasar App",
  "author": "Guavka",
  "private": true,
  "packageManager": "pnpm@7.8.0",
  "scripts": {
    "dev": "quasar dev",
    "build": "quasar build",
    "lint": "eslint . --fix",
    "test": "vitest",
    "test:e2e": "cypress open",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -I",
    "coverage": "vitest run --coverage",
    "cypress": "cypress open",
    "showConfig": "tsc --showConfig"
