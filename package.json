  "dependencies": {
    "@quasar/extras": "^1.15.1",
    "@vueuse/core": "^9.1.0",
    "@vueuse/head": "^0.7.9",
    "axios": "^0.27.2",
    "pinia": "^2.0.18",
    "quasar": "^2.7.7",
    "vue": "^3.2.37",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^6.0.1",
    "@quasar/app-vite": "^1.0.6",
    "@types/node": "^18.7.6",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "autoprefixer": "^10.4.8",
    "c8": "^7.12.0",
    "critters": "^0.0.16",
    "cross-env": "^7.0.3",
    "cypress": "^10.6.0",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^9.3.0",
    "happy-dom": "^6.0.4",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "pnpm": "^7.9.3",
    "postcss": "^8.1.0",
    "rollup": ">=2.50.0 <3.0.0",
    "taze": "^0.7.6",
    "typescript": "^4.7.4",
    "unplugin-auto-import": "^0.11.1",
    "unplugin-vue-components": "^0.22.4",
    "vite": ">=3.0.0 <4.0.0",
    "vite-plugin-inspect": "^0.6.0",
    "vitest": "^0.22.0",
    "vue-tsc": "^0.40.1",
    "workbox-build": "^6.5.4",
    "workbox-cacheable-response": "^6.5.4",
    "workbox-core": "^6.5.4",
    "workbox-expiration": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-strategies": "^6.5.4"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1",
    "pnpm": "^7.9.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,vue,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  }
}
